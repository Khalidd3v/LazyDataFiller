{
    "manifest_version": 3,
    "name": "LazyFormFiller",
    "version": "1.0",
    "description": "Fills forms with random data for developers, This extension was madey by LazyAlgo.com!",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
        "16": "icon_16.svg",
        "48": "icon_48.svg",
        "128": "icon_128.svg"
        }
  }